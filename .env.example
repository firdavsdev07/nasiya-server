# =================================
# SERVER CONFIGURATION
# =================================
NODE_ENV=development
PORT=3000

# =================================
# DATABASE CONFIGURATION
# =================================
# MongoDB connection string
MONGO_DB=mongodb://localhost:27017/nasiya_db

# =================================
# JWT CONFIGURATION
# =================================
# JWT kalitlarini o'zgartiring (kamida 16 ta belgi)
JWT_ACCESS_KEY=your_jwt_access_secret_key_here
JWT_REFRESH_KEY=your_jwt_refresh_secret_key_here

# =================================
# TELEGRAM BOT CONFIGURATION
# =================================
# Telegram BotFather'dan olingan bot token
BOT_TOKEN=your_telegram_bot_token_here
BOT_USERNAME=@your_bot_username
ENABLE_BOT=true

# =================================
# CORS CONFIGURATION
# =================================
# Frontend URL manzillari
DASHBOARD_HOST_URL=http://localhost:5173
BOT_HOST_URL=https://your-bot-domain.com
BOT_WEB_APP_URL=http://localhost:5174

# =================================
# SUPER ADMIN CONFIGURATION
# =================================
# Birinchi super admin foydalanuvchi ma'lumotlari
ADMIN_FIRSTNAME=Super
ADMIN_LASTNAME=Admin
ADMIN_PHONENUMBER=+998901234567
ADMIN_PASSWORD=admin123

# =================================
# OPTIONAL CONFIGURATIONS
# =================================
# Session secret kaliti (kamida 32 ta belgi tavsiya etiladi)
SESSION_SECRET=your_session_secret_key_here
MAX_FILE_SIZE=5242880
UPLOAD_PATH=./uploads
LOG_LEVEL=info
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# =================================
# LOGGING CONFIGURATION
# =================================
# Console loglarni yoqish/o'chirish
ENABLE_LOGS=true
