    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.13",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
